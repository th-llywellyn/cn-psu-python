   "source": [
    "n=int(input())\n",
    "i=1\n",
    "while i<=n:\n",
    "    j=1\n",
    "    while j<=i:\n",
    "        ch=chr(ord('A')+i-1)\n",
    "        print(ch,end='')            \n",
    "        j=j+1\n",
    "    print()\n",
    "    i=i+1"
   ]
