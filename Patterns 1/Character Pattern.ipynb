   "source": [
    "n=int(input())\n",
    "i=1\n",
    "while i<=n:\n",
    "    j=1\n",
    "    asc=ord('A')\n",
    "    while j<=i:\n",
    "        pp=asc+i-1\n",
    "        p=chr(pp+j-1)\n",
    "        print(p,end='')\n",
    "        j+=1\n",
    "    print()\n",
    "    asc+=1\n",
    "    i+=1"
   ]
  }
