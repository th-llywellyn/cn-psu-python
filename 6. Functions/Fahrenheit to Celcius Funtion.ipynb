    "def FAH_CEL(start,end,step):\n",
    "    for curr_temp in range(start, end+1, step):\n",
    "        c = 5/9 * (curr_temp-32)\n",
    "        print(curr_temp, \" \", int(c))\n",
    "   \n",
    "s = int(input())\n",
    "e = int(input())\n",
    "step = int(input())\n",
    "FAH_CEL(s,e,step)"
   ]
  }
