    "Sum of Even Numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "n=int(input())\n",
    "total=0\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "\n",
    "    # Check for even or not.\n",
    "    if((i % 2) == 0):\n",
    "        total = total + i\n",
    "print(total)"
   ]
  }
